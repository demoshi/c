#player,#player a{ color:#333; }
#player{ background:#ffffff; background-position:bottom; }
#player .mainImage{ background:url(https://i.imgur.com/OdYOjSx.png); }

#player #baseLeft{ width:93px; }
#player #baseRight{ width:145px;}
#player .button{  width:26px; }
#player #play{ background:url(https://i.imgur.com/T7qH7JH.png); }
#player #play,#player #pause{ left:26px; }
#player #previous{ left:0px; }
#player #next{ left:52px; }
#player #title{ left:90px;  }
#player #tooltip{ right:196px; }
#player #timer{ right:120px;  }
#player #list{ right:0px; }

#player #seeker{ right:320px; }
#player #seekerBase{ border-color:#ccc;  }
#player #seekerBuffered{ background-color:#4b575d; }
#player #seekerSliderBase{ background-color:#ccc; }

#player #volume{ width:65px; right:39px; }
#player #volumeSlider{ width:8px; background:url(https://i.imgur.com/HPpL5wH.png); }

#playlist,#playlist .module,#playlist h3{ border-color:#fff;}
#playlist .module{  color:#444; background-color:#fff; }
#playlist h3{color:#666; background-color:#fff; }
#playlist .even{background-color:#fff;}
#playlist .odd{ background-color:#fff;}
#playlist .focus{ background-color:#fff; color:#444; font-weight:bold}
